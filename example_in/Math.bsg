[Singleton]
class Math {
    max(a: Int, b: Int): Int {
        if(a > b) {
            return a;
        }
        return b;
    }

    min(a: Int, b: Int): Int {
        if(a < b) {
            return a;
        }
        return b;
    }

    clamp(a: Int, b: Int, c: Int): Int {
        return max(b, min(a, c));
    }
}